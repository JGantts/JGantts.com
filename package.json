{
  "name": "jgantts.com",
  "version": "1.0.10",
  "description": "jgantts.com",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run build && node dist/out/index.js",
    "prebuild": "if [ -d 'dist/' ]; then rm -Rf dist/ ; fi",
    "build": "tsc",
    "postbuild": "npm run copy-node_modules && npm run copy-src && npm run copy-others-excl-hidden",
    "copy-node_modules": "rsync -av node_modules/ dist/out/node_modules",
    "copy-src": "rsync -av --exclude '*.ts' src/ dist/out/src",
    "copy-others-excl-hidden": "rsync -av --exclude 'dist/' --exclude 'src/' --exclude 'node_modules' --exclude './.*' ./ dist/",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JGantts/jgantts.com.git"
  },
  "author": "Jacob Gantt",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JGantts/jgantts.com/issues"
  },
  "homepage": "https://github.com/JGantts/jgantts.com#readme",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jquery": "^3.5.14",
    "@types/node": "^17.0.5",
    "node": "14.18.3",
    "nodemon": "^2.0.18",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@types/mysql": "^2.15.21",
    "async": "^3.2.4",
    "body-parser": "^1.20.0",
    "express": "^4.18.1",
    "graceful-fs": "^4.2.10",
    "mysql": "^2.18.1"
  }
}
