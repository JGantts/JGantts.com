{
  "name": "jgantts.com",
  "version": "1.2.20",
  "description": "jgantts.com",
  "main": "out/index.js",
  "scripts": {
    "start": "npm run build && node index.js",
    "prebuild": "if [ -d 'dist/' ]; then rm -Rf dist/ ; fi",
    "build": "tsc",
    "postbuild": "npm run && npm run copy-src && npm run copy-package",
    "copy-src": "rsync -av --exclude '*.ts' src/ dist/out",
    "copy-package": "rsync -av ./package.json dist/package.json && rsync -av ./package-lock.json dist/package-lock.json",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JGantts/jgantts.com.git"
  },
  "author": "Jacob Gantt",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JGantts/jgantts.com/issues"
  },
  "homepage": "https://github.com/JGantts/jgantts.com#readme",
  "devDependencies": {
    "@types/mysql": "^2.15.21",
    "@types/express": "^4.17.13",
    "@types/jquery": "^3.5.14",
    "@types/node": "^17.0.5",
    "node": "14.18.3",
    "nodemon": "^2.0.18",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "async": "^3.2.4",
    "body-parser": "^1.20.0",
    "bootstrap": "^5.2.0-beta1",
    "compare-versions": "^4.1.3",
    "express": "^4.18.1",
    "graceful-fs": "^4.2.10",
    "log4js": "^6.5.2",
    "mysql": "^2.18.1",
    "node-cron": "^3.0.1",
    "tar": "^6.1.11",
    "uuid": "^8.3.2"
  }
}
